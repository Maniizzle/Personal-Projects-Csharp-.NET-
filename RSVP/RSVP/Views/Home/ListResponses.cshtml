@model IEnumerable<GuestResponse>
@{
    ViewData["Title"] = "ListResponses";
}


<h2> Here is the list of People attending the Party</h2>
<table>
    <thead>
        <tr>
            <th> Name </th>
            <th>Email </th>
            <th> Phone </th>
        </tr>
    </thead>
    <tbody>
        
        @foreach (GuestResponse r in Model)
        {
                <tr>
                    <td> @r.Name  </td>
                    <td> @r.Email </td>
                    <td> @r.Phone </td>
                </tr>
                   }
    </tbody>

</table>
